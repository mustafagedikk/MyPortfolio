
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Deneyim Ekle</h2>

<form method="post">

    <label>Başlangıç Yılı</label>
    <input type="text" name="StarYear" class="form-control" />
    <br />

    <label>Bitiş Yılı</label>
    <input type="text" name="AndYear" class="form-control" />
    <br />

    <label>Pozisyon</label>
    <input type="text" name="Title" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" name="Description" class="form-control" />
    <br />
    <label>Şirket</label>
    <input type="text" name="Company" class="form-control" />
    <br />
    <label>Lokasyon</label>
    <input type="text" name="Location" class="form-control" />
    <br />


    <button class="btn btn-secondary">Kaydet</button>

</form>